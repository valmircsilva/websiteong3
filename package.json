  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "minify:css": "cleancss -o dist/css/style.css css/style.css",
    "minify:js": "terser js/main.js -o dist/js/main.js -c -m",
    "minify:html:index": "html-minifier index.html -o dist/index.html --remove-comments --collapse-whitespace",
    "minify:html:cadastro": "html-minifier cadastro.html -o dist/cadastro.html --remove-comments --collapse-whitespace",
    "minify:html:inicio": "html-minifier inicio.html -o dist/inicio.html --remove-comments --collapse-whitespace",
    "minify:html:projetos": "html-minifier projetos.html -o dist/projetos.html --remove-comments --collapse-whitespace",
    "minify:html": "npm-run-all --parallel minify:html:*",
    "compress:images": "imagemin images/* --out-dir dist/images",
    "build": "npm-run-all --parallel minify:css minify:js minify:html compress:images"
  },
